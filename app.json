{
  "expo": {
    "name": "TravelQuest",
    "slug": "travelquest",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travelquest.app"
    },
    "android": {
      "package": "com.travelquest.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "scheme": "travelquest",
    "plugins": [
      "expo-location",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "c2ecce27-7f4c-4193-afb9-b7c7a30e1339"
      }
    }
  }
}